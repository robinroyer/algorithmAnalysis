             
def fusion(listes) :
    n0=len(listes[0])
    n1=len(listes[1])
    if n0==0:
        return listes[1]
    elif n1==0:
        return listes[0]
    elif (listes[0][0]<listes[1][0]):
        return [listes[0][0]]+fusion([listes[0][1:n0],listes[1]])
    else :
        return [listes[1][0]]+fusion([listes[0],listes[1][1:n1]])
def mergesort(seq):
    n=len(seq)
    if n<2 :
        return seq
    else :
        return fusion([mergesort(seq[0:int(n/2)]),mergesort(seq[int(n/2):n])])
